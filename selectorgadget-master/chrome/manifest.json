{
  "manifest_version": 2,
  "name": "MrRobot",
  "description": "데이터 수집을 위한 통합 크롤러 chrome extension.",
  "version": "1.1",
  "permissions": [
    "tabs", "http://*/*", "https://*/*"
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_title": "MrRobot"
  }
}
