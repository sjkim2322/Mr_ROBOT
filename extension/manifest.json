{
    "manifest_version": 2,

    "name": "Hello_World",
    "description": "Print Hello, World! message",
    "version": "1.0",
    "content_scripts": [
       {
         "matches": ["<all_urls>"],
         "js": ["jquery-3.2.1.js"]
       }
     ],
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },
    "permissions": [
        "tabs",
        "<all_urls>"
    ]
}
